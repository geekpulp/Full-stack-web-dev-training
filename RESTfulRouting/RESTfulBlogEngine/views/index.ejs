<% include ./partials/header %>



<%blogs.forEach(function(blog){%>
<div class="ui main text container segment">
  <div class="ui huge header">
    <h1>Blogs</h1>
    <h2>
      <%=blog.title%>
    </h2>
    <img src="<%=blog.image%>" alt="">
    <span>
      <%=blog.created%></span>
    <p><%=blog.body%></p>
  </div>
</div>
<% })%>

<% include ./partials/footer %>